fragment MessengerOverviewUser on User {
  ...MessengerBasicUser
  age
  albumPhotosUrl
  canReceiveMessages
  city
  distance
  gender
  id
  isOnline
  currentOnlineChannelName
  latestOnlineChannelName
  lastOnlineTime
  nick
  profilePicture {
    urlCustomSizeSquare(pixelDensity: $pixelDensity, size: 60)
    __typename
